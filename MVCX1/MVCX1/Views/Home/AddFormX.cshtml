@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@model MVCX1.Models.Players

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <p>@ViewBag.Title</p>
